package com.npci.dao;

import org.springframework.stereotype.Repository;

@Repository // spring creates the object of this class
// by default its id is same as class name but begins with lowercase
// i.e., employeeDaoV1 is the id
public class EmployeeDaoV1 implements EmployeeDao{

	@Override
	public void store() {
		System.out.println("---- store() inside V1 ----");
	}
	
}
